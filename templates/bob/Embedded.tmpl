<div class="widget_wrap" style="
  top: {{y}}px;
  left: {{x}}px;
  {% if width != -1 %}width: {{width}}px;{% end %}
  {% if height != -1 %}height: {{height}}px;{% end %}
  ">

  <iframe id="widget_body" src="{{file}}"
    style="width: 100%;
           height: 100%;
          ">
  </iframe>
</div>
